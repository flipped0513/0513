#include<stdio.h>
int main()
{
    char str1[] = {'a', 'c', 'd', 'z', '3','/0'};
    char str2[] = {'a', 'c', '/0', 'z', '3','d'};
    printf("sizeof 1=%d\n",sizeof(str1));
    printf("sizeof 2=%d\n",sizeof(str2));
    printf("strlen 1=%d\n",strlen(str1));
    printf("strlen 2=%d\n",strlen(str2));
    return 0;


}
